<!doctype html>
<html lang="en">

<head>
  <title>Title</title>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <!-- Bootstrap CSS v5.2.0-beta1 -->
  <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">
  <link rel="stylesheet" href="//cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css">
  <link rel="stylesheet" href="/css/style.css">
  

</head>

<body>

	<%- body  %> 

  
  <!-- Bootstrap JavaScript Libraries -->
  <script src="//code.jquery.com/jquery-3.6.1.js"></script>
  <script src="//cdn.jsdelivr.net/npm/@popperjs/core@2.11.5/dist/umd/popper.min.js">
  </script>
  <script src="//cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.min.js">
  </script>
 <script src="//cdn.datatables.net/1.12.1/js/jquery.dataTables.min.js"></script>
  <script>

      $('.myTable').DataTable();

      const preloadstudent= document.querySelector('.preload-student');
      const image_upload= document.getElementById("image_upload");

      if(image_upload){
        image_upload.onchange=(e)=>{
        const imageLink= URL.createObjectURL(e.target.files[0]);
       
        preloadstudent.setAttribute('src',imageLink);

      }

      }

      // delete images or data 

      const delete_btn= document.querySelectorAll('.delete-btn');
      
      if(delete_btn){
        delete_btn.forEach( item=>{
        item.onclick=(e)=>{
            const nmf= confirm('R you sure ?');

          if(nmf){
            return true;
          }else{
            return false;
          }

        }


      })
      }


  </script>
</body>

</html>